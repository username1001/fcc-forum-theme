{
  "manifest_version": 2,
  "name": "FCC-Forum Theme",
  "version": "1.0",

  "description": "A dark theme that's easy on your eyes",

  "content_scripts": [
    {
      "matches": ["*://*.forum.freecodecamp.org/*"],
      "js": ["script.js"]
    }
  ],
  "browser_action": {
    "default_icon": "favicon.png"
  },
  "icons": {
    "48": "favicon.png"
  }
}
